{
  "name": "@unmock/jaymock",
  "version": "1.0.8",
  "description": "Minimal fake JSON test data generator",
  "main": "index.js",
  "scripts": {
    "test": "xo && ava && tsd",
    "coverage": "nyc npm test",
    "coverage:upload": "nyc report --reporter=lcov && codecov",
    "coverage:check": "nyc report && nyc check-coverage --lines 90 --functions 90 --branches 85"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unmock/jaymock.git"
  },
  "xo": {
    "semicolon": false,
    "rules": {
      "max-depth": [
        "error",
        5
      ]
    }
  },
  "author": "unmock",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unmock/jaymock/issues"
  },
  "homepage": "https://github.com/unmock/jaymock#readme",
  "dependencies": {
    "faker": "^4.1.0",
    "lodash.clonedeep": "^4.5.0"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "codecov": "^3.6.1",
    "nyc": "^14.1.1",
    "supervillains": "^3.0.0",
    "tsd": "^0.11.0",
    "xo": "^0.25.3"
  },
  "keywords": [
    "mock",
    "json",
    "fake",
    "data"
  ]
}
